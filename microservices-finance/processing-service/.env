# Processing Service Environment Variables
PORT=3003
NODE_ENV=development

# Database
MONGODB_URI=mongodb://mongo:27017/finance_processing

# Redis Queue
REDIS_URL=redis://redis:6379
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=

# JWT
JWT_SECRET=your-super-secret-jwt-key-change-in-production
JWT_EXPIRY=24h

# Worker Configuration
MAX_WORKERS=4
QUEUE_CONCURRENCY=5

# PDF Generation
PDF_TIMEOUT=30000
MAX_PDF_SIZE=10485760

# Performance
MEMORY_LIMIT=512
CPU_LIMIT=1

# Monitoring
METRICS_PORT=3004
